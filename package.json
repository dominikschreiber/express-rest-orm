{
  "name": "express-rest-orm",
  "version": "0.1.1",
  "description": "rest interface generation for sequelize models",
  "main": "lib/index.js",
  "scripts": {
    "test": "multi='spec=- travis-cov=-' ./node_modules/.bin/mocha --require blanket --reporter mocha-multi",
    "coverage": "./node_modules/.bin/mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dominikschreiber/express-rest-orm"
  },
  "keywords": [
    "rest",
    "rest-api",
    "orm",
    "express",
    "sequelize"
  ],
  "author": "Dominik Schreiber <dominik@dominikschreiber.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dominikschreiber/express-rest-orm/issues"
  },
  "homepage": "https://github.com/dominikschreiber/express-rest-orm",
  "dependencies": {
    "easyxml": "^1.0.0",
    "express": "^4.12.3",
    "lodash": "^3.8.0",
    "promise": "^7.0.1",
    "q": "^1.4.0",
    "sequelize": "^2.1.0",
    "sqlite3": "^3.0.7",
    "untyped": "^0.1.0",
    "yamljs": "^0.2.2"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "body-parser": "^1.12.3",
    "coveralls": "^2.11.2",
    "mocha": "^2.2.4",
    "mocha-lcov-reporter": "0.0.2",
    "mocha-multi": "^0.7.1",
    "supertest": "^0.15.0",
    "travis-cov": "^0.2.5"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    },
    "travis-cov": {
      "threshold": 100
    }
  }
}
