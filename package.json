{
  "name": "express-rest-orm",
  "version": "0.0.3",
  "description": "rest interface generation for sequelize models",
  "main": "lib/index.js",
  "scripts": {
    "test": "multi='spec=- travis-cov=-' ./node_modules/.bin/mocha --require blanket --reporter mocha-multi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dominikschreiber/express-rest-orm"
  },
  "keywords": [
    "rest",
    "rest-api",
    "orm",
    "express",
    "sequelize"
  ],
  "author": "Dominik Schreiber <dominik@dominikschreiber.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dominikschreiber/express-rest-orm/issues"
  },
  "homepage": "https://github.com/dominikschreiber/express-rest-orm",
  "dependencies": {
    "easyxml": "^1.0.0",
    "express": "^4.12.3",
    "lodash": "^3.8.0",
    "promise": "^7.0.1",
    "sequelize": "^2.1.0",
    "sqlite3": "^3.0.7",
    "yamljs": "^0.2.2"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "body-parser": "^1.12.3",
    "mocha": "^2.2.4",
    "mocha-multi": "^0.7.1",
    "supertest": "^0.15.0",
    "travis-cov": "^0.2.5"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    },
    "travis-cov": {
      "threshold": 70
    }
  }
}
